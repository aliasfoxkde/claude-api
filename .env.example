# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Worker Configuration
WORKER_NAME=puter-claude-api
WORKER_SUBDOMAIN=your-subdomain

# Documentation Site Configuration
DOCS_PROJECT_NAME=puter-claude-docs
DOCS_CUSTOM_DOMAIN=your-docs-domain.com

# API Configuration
API_BASE_URL=https://your-worker.your-subdomain.workers.dev
DOCS_BASE_URL=https://your-docs-site.pages.dev

# Development Configuration
NODE_ENV=development
VITE_API_BASE_URL=http://localhost:8787

# GitHub Configuration (for CI/CD)
GITHUB_TOKEN=your_github_token_here

# Optional: Custom Domain Configuration
CUSTOM_DOMAIN=api.yourdomain.com
DOCS_CUSTOM_DOMAIN=docs.yourdomain.com

# Optional: Analytics Configuration
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
CLOUDFLARE_ANALYTICS_TOKEN=your_analytics_token

# Optional: Monitoring Configuration
SENTRY_DSN=your_sentry_dsn
DISCORD_WEBHOOK_URL=your_discord_webhook_url
